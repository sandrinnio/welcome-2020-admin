{
  "name": "@welcome/graphql",
  "version": "1.0.0",
  "main": "lib/index.js",
  "private": true,
  "license": "MIT",
  "scripts": {
    "type-check": "tsc --noEmit",
    "type-check:watch": "yarn run type-check -- --watch",
    "build": "yarn run build:types && yarn run build:js",
